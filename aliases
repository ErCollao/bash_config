# My personalized tooling instead of ls and such
alias l='ls -GCF'
alias la='ls -GCFa'
alias ll='ls -lhGF'
alias lla='ls -lhGFa'
alias lll='du -skh *'
alias vim.='vim .'
alias ww='vim ~/.wiki/index.md.wiki'
alias cd..='cd ..'
alias :q=exit
alias browse_hivemind='firefox localhost:5000'

# My quick access to servers
alias sshfairforce='ssh fkqkuhxkmu@fairforce.tech'
